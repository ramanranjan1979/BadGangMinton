<section id="joinus">
    <div id="contact-us" class="parallax">
        <div class="container">
            <div class="row">
                <div class="heading text-center col-sm-8 col-sm-offset-2 wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                    <h2>Join Us</h2>
                    <p>To join us, please enter your details</p>
                </div>
            </div>
            <div class="contact-form wow fadeIn" data-wow-duration="1000ms" data-wow-delay="600ms">
                <div class="row">
                    <div class="col-sm-12">
                        @using (Html.BeginForm("JoineMe", "Home", FormMethod.Post, new { ID = "FRM_BGB_JOIN_BG", @class = "form-horizontal", @autocomplete = "off" }))
                        {
                            @Html.AntiForgeryToken()
                            <div class="row  wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                                <div class="col-sm-12">

                                    <div class="form-group">
                                        <label for="FirstName" class="col-sm-2 control-label">First Name</label>
                                        <div class="col-sm-4" style="margin-bottom: 30px;">
                                            @Html.EditorFor(model => model.FirstName, new { htmlAttributes = new { placeholder = "John", @class = "form-control", maxlength = 20 } })
                                            @Html.ValidationMessageFor(model => model.FirstName, "", new { @class = "text-danger", @Style = "color:gold;" })
                                        </div>

                                        <label for="LastName" class="col-sm-2 control-label">Last Name</label>
                                        <div class="col-sm-4" style="margin-bottom: 30px;">
                                            @Html.EditorFor(model => model.LastName, new { htmlAttributes = new { placeholder = "Smith", @class = "form-control", maxlength = 20 } })
                                            @Html.ValidationMessageFor(model => model.LastName, "", new { @class = "text-danger", @Style = "color:gold;" })
                                        </div>
                                    </div>

                                    <div class="form-group">


                                        <label for="PrimaryContactNumber" class="col-sm-2 control-label">Contact Number</label>
                                        <div class="col-sm-4" style="margin-bottom: 30px;">
                                            @Html.EditorFor(model => model.PhoneNumber, new { type = "tel", htmlAttributes = new { @class = "form-control", maxlength = 15 } })
                                            @Html.ValidationMessageFor(model => model.PhoneNumber, "", new { @class = "text-danger", @Style = "color:gold;" })
                                        </div>

                                        <label for="PrimaryEmail" class="col-sm-2 control-label">Email Address</label>
                                        <div class="col-sm-4" style="margin-bottom: 30px;">
                                            @Html.EditorFor(model => model.PrimaryEmail, new { htmlAttributes = new { type = "email", placeholder = "johnsmith@hotmail.com", @class = "form-control", maxlength = 50 } })
                                            @Html.ValidationMessageFor(model => model.PrimaryEmail, "", new { @class = "text-danger", @Style = "color:gold;" })

                                        </div>
                                    </div>


                                    <div class="form-group">

                                        <label for="DOB" class="col-sm-2 control-label">Date Of Birth</label>
                                        <div class="col-sm-4" style="margin-bottom: 30px;">
                                            @Html.TextBoxFor(model => model.DateOfBirth, new { @readonly = "readonly", @class = "form-control", @style = "opacity: 0.35;color: gold;background-color: black;" })
                                            @Html.ValidationMessageFor(model => model.DateOfBirth, "", new { @class = "text-danger", @Style = "color:gold;" })
                                        </div>

                                        <label for="GenderTypeId" class="col-sm-2 control-label">Sex</label>
                                        <div class="col-sm-4" style="margin-bottom: 30px;">
                                            @Html.DropDownListFor(e => Model.Gender, Model.GenderSelectList, new { @class = "form-control" })
                                        </div>
                                    </div>


                                    <div class="form-group">
                                        <button type="submit" class="btn-submit animated infinite pulse">Join Now</button>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

